<tryton>
    <data>

        <record model="ir.ui.view" id="template_view_form">
            <field name="model">product.template</field>
            <field name="inherit" ref="product.template_view_form"/>
            <field name="arch" type="xml">
                <![CDATA[
                <data>
                    <xpath
                        expr="/form/notebook"
                        position="inside">
                        <page string="Manufacturer" col="3" id="manufacturer">
                            <label name="manufacturer"/>
                            <field name="manufacturer"/>
                            <newline/>
                            <label name="manufacturer_name"/>
                            <field name="manufacturer_name"/>
                            <newline/>
                            <label name="manufacturer_code"/>
                            <field name="manufacturer_code"/>
                        </page>
                    </xpath>
                </data>
                ]]>
            </field>
        </record>

    </data>
</tryton>
